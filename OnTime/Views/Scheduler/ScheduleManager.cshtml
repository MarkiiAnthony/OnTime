@{ 
    ViewBag.Title = "Schedule";

}

<h3>Upcoming Shifts</h3>
<hr/>

@if (ViewBag.EventList != null)
{
    <table class="table table-hover">
        <tr>
            <th>Title</th>
        </tr>
        @foreach (var item in ViewBag.EventList)
        {
                    <tr>
                        <td>@item</td>
                    </tr>
        }
    </table>



}
else
{
    <div class="alert alert-danger">No Schedule</div>
}